{
    "name": "elic/staades-sdk-php",
    "description": "A PHP library to send metrics to staades.net",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "staades",
        "analytics",
        "metrics"
    ],
    "require": {
        "php":           ">=5.3.3",
        "guzzle/guzzle": "~3.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7.0"
    },
    "homepage": "http://www.staades.net",
    "autoload": {
        "psr-4": {
            "Staades\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Staades\\Tests\\": "tests/Tests"
        }
    },
    "authors": [
        {
            "name": "Matthias Lill",
            "email": "matthias@staades.net"
        }
    ]
}